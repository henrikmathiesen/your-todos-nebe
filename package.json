{
  "name": "your-todos-nobe",
  "version": "1.0.0",
  "homepage": "http://henrikmathiesen.github.io/your-todos-nobe",
  "private": true,
  "devDependencies": {
    "del": "2.2.2",
    "gh-pages": "0.12.0",
    "gulp": "3.9.1",
    "gulp-angular-templatecache": "2.0.0",
    "gulp-autoprefixer": "3.1.1",
    "gulp-clean-css": "2.0.13",
    "gulp-concat": "2.6.0",
    "gulp-connect": "5.0.0",
    "gulp-eslint": "3.0.1",
    "gulp-if": "2.0.1",
    "gulp-inject": "4.1.0",
    "gulp-less": "3.1.0",
    "gulp-ng-annotate": "2.0.0",
    "gulp-rev": "7.1.2",
    "gulp-size": "2.1.0",
    "gulp-sourcemaps": "2.2.0",
    "gulp-strip-debug": "1.1.0",
    "gulp-uglify": "2.0.0",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-jasmine": "1.0.2",
    "karma-phantomjs-launcher": "1.0.2",
    "run-sequence": "1.2.2",
    "typings": "2.1.0",
    "uglify-save-license": "0.4.1",
    "yargs": "6.3.0"
  },
  "dependencies": {
    "angular": "1.5.8",
    "angular-mocks": "1.5.8",
    "fastclick": "1.0.6",
    "font-awesome": "4.7.0",
    "jquery": "3.1.1"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "jquery": true
    },
    "globals": {
      "jQuery": true,
      "$": true,
      "angular": true,
      "FastClick": true
    },
    "rules": {
      "no-console": 0,
      "no-debugger": 0,
      "no-unused-vars": 0
    }
  },
  "scripts": {
    "postinstall": "npm run typings",
    "typings": "typings install",
    "start": "gulp",
    "test": "gulp test",
    "prod": "gulp build --prod",
    "serve": "gulp connect",
    "deploy": "npm run prod && gh-pages -d bld",
    "clean-gh-pages": "rm -rf node_modules/gh-pages/.cache"
  }
}
